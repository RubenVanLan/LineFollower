
#define INTERNAL_LED 2

void setup() {
  pinMode(INTERNAL_LED, OUTPUT);

  // Startup sequentie knipperen ingebouwde LED
  for (int i = 0; i < 5; i++) {
    digitalWrite(INTERNAL_LED, HIGH);
    delay(250);
    digitalWrite(INTERNAL_LED, LOW);
    delay(250);
  }
  delay(1000); // Pauze na startup sequentie
}


void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(INTERNAL_LED, HIGH);
    delay(500);
    digitalWrite(INTERNAL_LED, LOW);
    delay(500);
}


